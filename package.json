{
  "name": "demo",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.2.20",
  "scripts": {
    "dev": "bun db:migrate && react-router dev",
    "build": "bun db:gen:prod && react-router build",
    "build:local": "bun env:check && bun db:migrate:deploy && bun db:gen:prod && react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "start:local": "dotenv -e .env react-router-serve ./build/server/index.js",
    "start:prod": "dotenv -e .env.prod react-router-serve ./build/server/index.js",
    "check": "biome check && bun rr:check",
    "write": "bun biome:format && bun biome:lint",
    "biome:format": "biome format --write .",
    "biome:lint": "biome lint --write .",
    "rr:check": "react-router typegen && tsc --noEmit",
    "env:check": "bun run app/modules/env/env.server.ts",
    "dep:check": "depcheck",
    "dep:up": "taze && taze major",
    "dep:up:write": "taze -w && taze major -w",
    "dep:sly": "sly add --overwrite",
    "dep:svg": "svgr --ignore-existing --typescript --jsx-runtime automatic --filename-case kebab --icon --title-prop --out-dir app/components/svgs public/svgs/*.svg",
    "db:up": "docker compose up -d",
    "db:up:prod": "docker compose -f docker-compose.prod.yml up -d",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:prod": "dotenv -e .env.prod bun db:migrate:deploy",
    "db:gen": "prisma generate --no-hints",
    "db:gen:prod": "bun db:gen",
    "db:studio": "prisma studio --browser none",
    "db:studio:prod": "dotenv -e .env.prod bun db:studio",
    "db:reset": "prisma migrate reset --skip-seed",
    "db:reset:prod": "dotenv -e .env.prod bun db:reset --skip-seed",
    "db:seed": "prisma db seed",
    "db:seed:prod": "dotenv -e .env.prod bun db:seed",
    "db:prune": "bun db:reset && bun db:seed",
    "auth:gen": "bunx @better-auth/cli generate --config app/server/better-auth.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "bun ultracite format"
    ]
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.2",
    "@conform-to/react": "^1.8.2",
    "@conform-to/zod": "^1.8.2",
    "@date-fns/utc": "^2.1.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/is-prop-valid": "^1.3.1",
    "@polar-sh/better-auth": "^1.1.0",
    "@polar-sh/sdk": "^0.34.13",
    "@prisma/adapter-pg": "^6.14.0",
    "@prisma/client": "^6.14.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@react-router/node": "^7.8.1",
    "@react-router/serve": "^7.8.1",
    "@shikijs/transformers": "^3.11.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tanem/react-nprogress": "^5.0.55",
    "@tanstack/react-query": "^5.85.5",
    "@trpc/client": "^11.5.0",
    "@trpc/server": "^11.5.0",
    "@trpc/tanstack-react-query": "^11.5.0",
    "@vercel/react-router": "^1.2.2",
    "better-auth": "^1.3.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "color": "^5.0.0",
    "country-data-list": "^1.5.4",
    "culori": "^4.0.2",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "flagpack-core": "^2.0.0",
    "gsap": "^3.13.0",
    "html-react-parser": "^5.2.6",
    "input-otp": "^1.4.2",
    "isbot": "^5.1.30",
    "libphonenumber-js": "^1.12.13",
    "motion": "^12.23.12",
    "nanoid": "^5.1.5",
    "next-themes": "^0.4.6",
    "pluralize": "^8.0.0",
    "qrcode": "^1.5.4",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-circle-flags": "^0.0.23",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.1.1",
    "react-fast-marquee": "^1.6.5",
    "react-flagpack": "^2.0.6",
    "react-hook-form": "^7.62.0",
