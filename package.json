    "@react-router/dev": "^7.8.1",
    "@sly-cli/sly": "^2.1.1",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.12",
    "@types/bun": "^1.2.20",
    "@types/color": "^4.2.0",
    "@types/culori": "^4.0.0",
    "@types/node": "^22.17.2",
    "@types/pg": "^8.11.10",
    "@types/pluralize": "^0.0.33",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "depcheck": "^1.4.7",
    "@dotenvx/dotenvx": "^1.49.0",
    "dotenv-cli": "^10.0.0",
    "husky": "^9.1.7",
    "jsrepo": "^2.4.5",
    "lint-staged": "^16.1.5",
    "prisma": "^6.14.0",
    "shadcn": "^2.10.0",
    "tailwindcss": "^4.1.12",
    "taze": "^19.3.0",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "ultracite": "^5.2.5",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^1.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "zod-prisma-types": "^3.2.4"
  },
  "overrides": {
    "better-sqlite3": "npm:noop@^1.0.0"
  }
}
